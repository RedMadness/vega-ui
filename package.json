{
  "name": "vega-ui",
  "version": "0.1.6",
  "description": "Vue 3 UI library for rapid prototyping.",
  "keywords": [
    "vue",
    "layout",
    "sidebar",
    "ui",
    "vega"
  ],
  "license": "MIT",
  "homepage": "https://github.com/RedMadness/vega-ui",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vega.umd.cjs",
  "module": "./dist/vega.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vega.js",
      "require": "./dist/vega.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6",
    "vite-plugin-dts": "^3.9.1"
  }
}
